type hal_lineage_fod_sm8350, domain;
hal_server_domain(hal_lineage_fod_sm8350, hal_lineage_fod)

type hal_lineage_fod_sm8350_exec, exec_type, vendor_file_type, file_type;
init_daemon_domain(hal_lineage_fod_sm8350)

# Allow access to the HALs
hal_client_domain(hal_lineage_fod_sm8350, hal_fingerprint)

# Allow binder communication with hal_fingerprint_default
binder_call(hal_lineage_fod_sm8350, hal_fingerprint_default)

# Allow hal_lineage_fod_sm8350 to hal_fingerprint_hwservice
allow hal_lineage_fod_sm8350 hal_fingerprint_hwservice:hwservice_manager find;

allow hal_lineage_fod_sm8350 sysfs_fod:file r_file_perms;
